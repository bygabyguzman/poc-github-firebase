{
  "name": "poc-github-firebase",
  "version": "1.0.0",
  "description": "github actions react firebase",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --env config=prod webpack.config.js",
    "dev:build": "webpack --mode development --env config=dev --config webpack.config.js",
    "qa:build": "webpack --mode production  --env config=qa --config webpack.config.js",
    "dev:deploy": "firebase deploy -P dev",
    "qa:deploy": "firebase deploy -P qa",
    "linting": "eslint .eslintrc src/ --fix ",
    "start:prod": "webpack serve  --mode production --config webpack.config.js ",
    "start": "webpack serve --open --mode development --env config=dev --config webpack.development.config.js ",
    "qa:start": "webpack serve --open --mode development --env config=qa --config webpack.config.js ",
    "test": "jest -u --config jest.config.js",
    "watch:test": "jest -u --watchAll --config jest.config.js",
    "default:deploy": "firebase deploy -P default",
    "emulate": "firebase emulators:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gghgaby/poc-github-firebase.git"
  },
  "keywords": [
    "react",
    "github",
    "actions",
    "globant",
    "firebase"
  ],
  "author": "gghgaby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gghgaby/poc-github-firebase/issues"
  },
  "homepage": "https://github.com/gghgaby/poc-github-firebase#readme",
  "dependencies": {
    "babel-loader": "^8.2.2",
    "connected-react-router": "^6.9.1",
    "firebase": "^9.1.1",
    "milligram": "^1.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "css-loader": "^5.1.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.3.0",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}